!!!
%html
  %head
    %meta{content: "text/html; charset=UTF-8", "http-equiv": "Content-Type"}/
    %title Portfolio
    = csrf_meta_tags
    = csp_meta_tag
    %meta{content: "MsGMviSmnIXYhLTdfcWpeGPmxKnZ9vCwSWZR-sPlGJs", name: "google-site-verification"}/
    = javascript_include_tag "https://maps.googleapis.com/maps/api/js?key=#{Rails.application.credentials.development[:gmaps][:API_KEY]}"
    = stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload'
    = javascript_include_tag 'application', 'data-turbolinks-track': 'reload'
  %body.boxed
    = render 'front/sections/header/header'
    = yield
